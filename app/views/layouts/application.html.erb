<!DOCTYPE html>
<html>
<head>
  <title>AssignmentCrud</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body data-no-turbolink>

  <h3 style="color:green">
    <%= flash[:success] %>
  </h3>

  <h3 style="color:red">
    <%= flash[:error] %>
  </h3>

  <%= yield %>

</body>
</html>
