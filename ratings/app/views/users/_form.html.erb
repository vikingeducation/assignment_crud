<%= form_for(@user) do |f| %>
  <div>
    <div class="form-group">
      <h4><%= f.label "First Name" %></h4>
      <h4><%= f.text_field :first_name, class: "form-control"  %></h4>
    </div>

    <div class="form-group">
      <h4><%= f.label "Last Name" %></h4>
      <h4><%= f.text_field :last_name, class: "form-control"  %></h4>
    </div>

    <div class="form-group">
      <h4><%= f.label "Username" %></h4>
      <h4><%= f.text_field :user_name, class: "form-control"  %></h4>
    </div>

    <div class="form-group">
      <h4><%= f.label "Email" %></h4>
      <h4><%= f.email_field :email, class: "form-control" %></h4>
    </div>

    <div class="form-group">
      <h4><%= f.label "Password" %></h4>
      <h4><%= f.text_field :password, class: "form-control" %></h4>
    </div>

    <div class="form-group">
      <h4><%= f.submit "Submit", class: "btn btn-success"  %></h4>
    </div>
  </div>
<% end %>
