<%= form_for(@user) do |f| %>
  <ul>
    <li>
      <%= f.label "First Name" %>
      <%= f.text_field :first_name %>
    </li>
    <li>
      <%= f.label "Last Name" %>
      <%= f.text_field :last_name %>
    </li>
    <li>
      <%= f.label "Email" %>
      <%= f.text_field :email %>
    </li>
    <li>
      <%= f.submit %>
    </li>
  </ul>
<% end %>
