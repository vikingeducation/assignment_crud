<h1>All Users</h1>

<ul>
<% @users.each do |user| %>
  <li><%= link_to "#{user.first_name} #{user.last_name}", user_path(user.id) %>
  <%= "#{user.email}" %>
  <%= link_to "Edit", edit_user_path(user.id) %>
  <%= link_to "Delete", user_path(user.id), method: :delete, data: { confirm: "Really delete this user?"} %></li>
<% end %>
</ul>
